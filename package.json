{
    "name": "take-a-break-",
    "displayName": "Take a Break!",
    "description": "",
    "version": "0.0.1",
    "publisher": "MNBrod",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.startTimerBreak",
        "onCommand:extension.gitCommit"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.startTimerBreak",
                "title": "Take a Break!"
            }
        ],
        "keybindings": [
            {
                "command": "extension.gitCommit",
                "key": "cmd+shift+g"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "eslint": "^3.19.0",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "eslint-config-fullstack": "^3.0.0",
        "eslint-plugin-react": "^7.1.0",
        "simple-git": "^1.75.0"
    }
}
